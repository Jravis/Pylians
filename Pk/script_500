#!/bin/bash
#This script is used to generate the P(k) of the CDM, NU, DM and halos from the
#new simulations run by Simeon: 500 Mpc/h 512^3 CDM and 512^3 NU particles with
#the tree ON for the neutrinos.

dims=1024

mass_interval=1
min_mass=2.0e3
max_mass=3.0e5


############################## CDM ###################################

######## z=0 ########
#snapshot_fname=/data1/villa/b500p512nu0z99tree/snapdir_017/snap_017
#groups_fname=/data1/villa/b500p512nu0z99tree
#groups_number=17
snapshot_fname=/disk/disksom2/villa/b500p512nu0z99tree/snapdir_017/snap_017
groups_fname=/disk/disksom2/villa/b500p512nu0z99tree
groups_number=17

f_CDM=Pk_DM_0.0_z=0.dat
f_NU=A
f_DM=A
f_CDM_NU=A 
f_halos=Pk_halos_0.0_z=0.dat
f_CDM_halos=Pk_DM-halos_0.0_z=0.dat
f_DM_halos=A

obj=CDM

python Pk.py $snapshot_fname $groups_fname $groups_number $obj $mass_interval $min_mass $max_mass $dims $f_CDM $f_NU $f_DM $f_CDM_NU $f_halos $f_CDM_halos $f_DM_halos

obj=halos

python Pk.py $snapshot_fname $groups_fname $groups_number $obj $mass_interval $min_mass $max_mass $dims $f_CDM $f_NU $f_DM $f_CDM_NU $f_halos $f_CDM_halos $f_DM_halos

obj=CDM-halos

python Pk.py $snapshot_fname $groups_fname $groups_number $obj $mass_interval $min_mass $max_mass $dims $f_CDM $f_NU $f_DM $f_CDM_NU $f_halos $f_CDM_halos $f_DM_halos

######## z=0.5 ########
#snapshot_fname=/data1/villa/b500p512nu0z99tree/snapdir_012/snap_012
#groups_fname=/data1/villa/b500p512nu0z99tree
#groups_number=12
